[Unit]
Description=Plobin OCR Reader Service
Requires=docker.service
After=docker.service
StartLimitIntervalSec=0

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/home/gupsa/문서/Host/40001/api.ocr.plobin.com
ExecStart=/usr/bin/docker compose up -d
ExecStop=/usr/bin/docker compose down
TimeoutStartSec=0
User=gupsa
Group=gupsa

[Install]
WantedBy=multi-user.target